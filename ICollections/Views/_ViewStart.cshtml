@using ICollections.Data.Interfaces
@using ICollections.Models
@inject IUserRepository UserRepository;
@inject ICollectionRepository CollectionRepository;
@inject IItemRepository ItemRepository;
@inject ILikeRepository LikeRepository;
@{
    Layout = "_Layout";
    ViewBag.userData = UserRepository;
    ViewBag.collectionData = CollectionRepository;
    ViewBag.itemData = ItemRepository;
    ViewBag.likeData = LikeRepository;
}