@using ICollections.Data.Interfaces
@using ICollections.Models
@using ICollections.Services.Interfaces
@inject IUserRepository UserRepository;
@inject ICollectionRepository CollectionRepository;
@inject IItemRepository ItemRepository;
@inject ILikeRepository LikeRepository;
@inject IUserService UserService;
@inject ICollectionService CollectionService;
@inject IItemService ItemService;  
@inject ICommentService CommentService
@inject IRoleService RoleService
@inject ITagService TagService;
@{
    Layout = "_Layout";
    ViewBag.userService = UserService;
    ViewBag.collectionService = CollectionService;
    ViewBag.commentService = CommentService;
    ViewBag.itemService = ItemService;
    ViewBag.roleService = RoleService;
    ViewBag.tagService = TagService;
}